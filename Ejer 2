#include <iostream>
using namespace std;

int main()
{
    float NF;
    
    cout << "Ingrese su nota (sobre 100): ";
    cin >> NF;
    
    if (NF >= 0 && NF < 51) {
        cout << "Usted esta reprobado: ";
    }
    else if (NF >= 51 && NF <= 100) {
        cout << "Usted esta aprobado: ";
    }
    else {
        cout << "Error. Numero no valido";
    }
    return 0;
}
